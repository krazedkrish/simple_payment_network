<div class="result-container">
  <div class="result">
    <div class="table">
      <table class="pretty">
        <tr>
          <th>First Name </th>
          <th>Last Name</th>
          <th>Date of Birth</th>
          <th>Teams Joined</th>
          <th>Walled ID</th>
          <th>Balance</th>
          <th>Actions</th>
        </tr>
        <% @users.each do |u| %>
          <tr>
            <td><%= u.first_name %></td>
            <td><%= u.last_name %></td>
            <td><%= u.dob %></td>
            <td><%= u.teams.count %></td>
            <td><%= u.wallet.id %></td>
            <td><%= u.wallet.balance %></td>
            <td><%= link_to "View", u %>
          </tr>
        <% end %>
      </table>
    </div>

    <div class="paginate">
      <%= paginate @users %>
    </div>

  </div>
</div>